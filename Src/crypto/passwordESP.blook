if (stateNode.state.stage == "hack") Array.prototype.forEach.call(queryElement('div[class*=buttonContainer]').children, button => {
    if (button.innerText == stateNode.state.correctPassword) return;
    button.style.outlineColor = "rgba(255, 64, 64, 0.8)";
    button.style.backgroundColor = "rgba(255, 64, 64, 0.8)";
    button.style.textShadow = "0 0 1px #f33";
});